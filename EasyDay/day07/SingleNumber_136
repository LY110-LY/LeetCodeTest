package easyday.day07;

import java.util.Arrays;

//136. 只出现一次的数字
public class SingleNumber_136 {
    public int singleNumber(int[] nums) {
        int len = nums.length;
        Arrays.sort(nums);
        for (int i=0;i<len;i+=2){
            if (i==len-1){
                return nums[i];
            }
            if (nums[i]==nums[i+1]){
                continue;
            }else {
                return nums[i];
            }
        }
        return 0;
    }
}
